<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.idle.togeduck.view.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_ahyoungFragment"
            app:destination="@id/ahyoungFragment" />
        <action
            android:id="@+id/action_mainFragment_to_jiwooFragment"
            app:destination="@id/jiwooFragment" />
        <action
            android:id="@+id/action_mainFragment_to_jichanFragment"
            app:destination="@id/jichanFragment" />
        <action
            android:id="@+id/action_mainFragment_to_FavoriteSettingFragment"
            app:destination="@id/FavoriteSettingFragment" />
        <action
            android:id="@+id/action_mainFragment_to_bottomAppbarFragment"
            app:destination="@id/bottomAppbarFragment" />
    </fragment>

    <fragment
        android:id="@+id/ahyoungFragment"
        android:name="com.idle.togeduck.view.AhyoungFragment"
        android:label="ahyoung_fragment"
        tools:layout="@layout/fragment_ahyoung" >
    </fragment>

    <fragment
        android:id="@+id/jiwooFragment"
        android:name="com.idle.togeduck.view.JiwooFragment"
        android:label="jiwoo_fragment"
        tools:layout="@layout/fragment_jiwoo" >
    </fragment>

    <fragment
        android:id="@+id/jichanFragment"
        android:name="com.idle.togeduck.view.JichanFragment"
        android:label="jichan_fragment"
        tools:layout="@layout/fragment_jichan" >
    </fragment>

    <fragment
        android:id="@+id/FavoriteSettingFragment"
        android:name="com.idle.togeduck.view.FavoriteSettingFragment"
        android:label="favorite_setting_fragment"
        tools:layout="@layout/fragment_favorite_setting" >
    </fragment>


    <fragment
        android:id="@+id/bottomAppbarFragment"
        android:name="com.idle.togeduck.view.BottomAppbarFragment"
        android:label="bottom_appbar_fragment"
        tools:layout="@layout/fragment_bottom_appbar">
    </fragment>


</navigation>